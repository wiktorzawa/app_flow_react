{
  "type": "module",
  "name": "flowbite-react-admin-dashboard",
  "version": "1.1.0",
  "descrption": "flowbite-react admin dashboard",
  "homepage": "https://github.com/themesberg/flowbite-react-admin-dashboard",
  "bugs": "https://github.com/themesberg/flowbite-react-admin-dashboard/issues",
  "license": "UNLICENSED",
  "author": "Conner Davis <cdavis@scu.edu>",
  "repository": {
    "type": "git",
    "url": "https://github.com/themesberg/flowbite-react-admin-dashboard.git"
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite --host",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "preview": "vite preview --host",
    "start": "node dist/server.js",
    "clean": "rm -rf node_modules",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "turbo:build": "turbo run build",
    "turbo:dev": "turbo run dev",
    "turbo:test": "turbo run test"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8",
    "yarn": ">= 1"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "apexcharts": "^4.0.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "classnames": "^2.3.2",
    "clsx": "^2.1.1",
    "express-session": "^1.18.1",
    "flowbite": "^3.1.2",
    "flowbite-react": "^0.11.8",
    "react-apexcharts": "^1.4.1",
    "react-icons": "^5.5.0",
    "react-sortablejs": "^6.1.4",
    "sortablejs": "^1.15.0",
    "svgmap": "^2.10.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.10",
    "@types/express-session": "^1.18.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/sortablejs": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.46.0",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "5.5.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-vitest": "^0.5.4",
    "next": "^15.3.3",
    "postcss": "^8.5.6",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.14.2",
    "tailwindcss": "^4.1.10",
    "turbo": "^2.5.4",
    "typescript": "^5.3.3",
    "vite": "^6.3.5",
    "vite-plugin-tailwind": "^0.0.4",
    "vitest": "^3.1.3"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/strict",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:tailwindcss/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": [
        "./tsconfig.json",
        "./tsconfig.node.json"
      ]
    },
    "root": true,
    "rules": {
      "@typescript-eslint/consistent-type-imports": "error",
      "react/react-in-jsx-scope": "off",
      "react/no-unescaped-entities": "off",
      "tailwindcss/classnames-order": [
        "error",
        {
          "callees": [
            "classnames"
          ],
          "config": "tailwind.config.cjs"
        }
      ],
      "tailwindcss/no-custom-classname": [
        "error",
        {
          "config": "tailwind.config.cjs"
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "packageManager": "npm@10.2.4"
}
